{
  "version": 2,
  "builds": [
    {
      "src": "flask/app.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.12" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "flask/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "flask/app.py"
    }
  ]
}
